# This is a syntax definition file for use by GNU source-highlight for the
# formal syntax defintion "syntax" sections of phdl documentation.
# See <http://www.gnu.org/software/src-highlite> for more information.

# Comments
comment start "#"

# Keywords
keyword = "⇒|→"

# Rules
type = '[[:alnum:]_]+'

# Literals
environment string delim "\"" "\"" begin
	specialchar = '\\.'
end

# Regular Expressions
environment regexp delim "/" "/" begin
	regexp = '\\/'
end

# Symbols
symbol = '[^[:alnum:]]'
