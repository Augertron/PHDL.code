// a tantalum capacitor declaration
device tant_cap_3528 {
    attr refPrefix = "C";
    attr value = "";
    attr package = "TANT3528";
    attr mfgr = "AVX";
    attr partNumber = "TCJB107M006R0045";
    attr cost = "1.55";
    pin pos = {1};
