device mount_hole_125 {
    attr refPrefix = "MTG";
    attr package = "MOUNT_HOLE_125";
    attr cost = "0.00";
    attr mfgr = "";
    pin pin1 = {1};
}

device LT3022 {
    attr refPrefix = "U";
    attr package = "LT3022_MSOP16";
    attr pinCount = "17";
    attr value = "";
    attr cost = "6.10";
    attr mfgr = "LTC";
    pin[1:5] NC = {1,2,8,15,16};
    pin[1:2] OUT = {3,4};
    pin ADJ_SENSE = {5};
    pin[1:2] AGND = {6,7};
    pin SHDN_N = {9};
    pin[1:2] PGND = {10,11};
    pin[1:3] IN = {12,13,14};
    pin GND_PAD = {17};
}

